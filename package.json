{
  "name": "urtext-piano",
  "productName": "Urtext Piano",
  "version": "0.1.0-alpha",
  "description": "Learn piano with real sheet music - Professional practice app with <20ms latency",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:3000 && electron .\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "electron:build": "npm run build && electron .",
    "build:main": "esbuild \"src/main/**/*.ts\" --platform=node --format=cjs --target=node16 --outdir=src/main",
    "build:preload": "npx esbuild src/main/preload.ts --bundle --platform=node --format=cjs --target=node16 --outfile=dist/preload.js --external:electron",
    "build:electron": "npm run build:main && npm run build:preload",
    "preelectron": "npm run build:electron",
    "clean": "npx rimraf dist out \"src/main/**/*.js\" \"src/main/**/*.js.map\" \"!src/main/preload.js\"",
    "start": "npm run build:electron && npm run build && electron-forge start",
    "package": "npm run clean && npm run build:electron && npm run build && electron-forge package",
    "make": "npm run clean && npm run build:electron && npm run build && electron-forge make",
    "make:win": "npm run make -- --platform=win32 --arch=x64",
    "publish": "npm run clean && npm run build:electron && npm run build && electron-forge publish",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "build:clean": "node build.js clean",
    "build:dev": "node build.js dev",
    "build:prod": "node build.js prod",
    "build:package": "node build.js package",
    "build:zip": "node build.js zip",
    "build:release": "node build.js release"
  },
  "keywords": [],
  "author": {
    "name": "Urtext Piano Team",
    "email": "dev@urtextpiano.io"
  },
  "license": "MIT",
  "dependencies": {
    "dexie": "^4.0.11",
    "electron-squirrel-startup": "^1.0.1",
    "fast-xml-parser": "^5.2.5",
    "opensheetmusicdisplay": "^1.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unzipper": "^0.12.3",
    "uuid": "^11.1.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron": "^1.4.38",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/unzipper": "^0.10.11",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^37.2.4",
    "esbuild": "^0.25.5",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "wait-on": "^8.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.46.1"
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node"
  }
}
